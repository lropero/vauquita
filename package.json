{
  "name": "vauquita",
  "version": "0.1.0",
  "description": "A lightweight batteries-included template for React apps.",
  "scripts": {
    "build": "npx webpack --config webpack.prod.js",
    "clean": "npx rimraf npm-*.log && npx rimraf package-lock.json && npx -p rimraf rimraf node_modules && npm cache verify",
    "continue": "npx webpack serve --config webpack.dev.js",
    "lint": "npx prettier-standard --lint",
    "nuke": "npm run clean && npm install",
    "prepare": "npx husky install",
    "start": "npx webpack serve --config webpack.dev.js --open",
    "upgrade": "npx upgradeps -n"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@reduxjs/toolkit": "^1.8.0",
    "i18next": "^21.6.12",
    "i18next-browser-languagedetector": "^6.1.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@svgr/webpack": "^6.2.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "dotenv": "^16.0.0",
    "dotenv-webpack": "^7.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "upgradeps": "^1.5.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "author": "Luciano Ropero <lropero@gmail.com>",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "*": "npx prettier-standard --lint"
  },
  "private": true
}
