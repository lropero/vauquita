{
  "name": "vauquita",
  "version": "0.1.0",
  "description": "A lightweight batteries-included template for React apps.",
  "scripts": {
    "build": "npx rimraf dist && npx webpack --config webpack.prod.js",
    "clean": "npx rimraf npm-*.log && npx rimraf package-lock.json && npx -p rimraf rimraf node_modules && npm cache verify",
    "lint": "npx prettier-standard --lint",
    "nuke": "npm run clean && npm install",
    "start": "npx webpack serve --config webpack.dev.js",
    "upgrade": "npx upgradeps -n && npm run upgrade:tailwind",
    "upgrade:tailwind": "npx tailwindcss-cli build -o public/tailwind.css && npx -p clean-css-cli cleancss -o public/tailwind.min.css public/tailwind.css && npx rimraf public/tailwind.css"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "antd": "^4.10.3",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.5",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "upgradeps": "^1.4.2",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "author": "Luciano Ropero <lropero@gmail.com>",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "license": "UNLICENSED",
  "private": true
}
