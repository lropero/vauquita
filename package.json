{
  "name": "vauquita",
  "version": "0.1.1",
  "description": "A lightweight batteries-included template for React apps.",
  "scripts": {
    "build": "npx rimraf dist && npx webpack --config webpack.prod.js",
    "clean": "npx rimraf npm-*.log && npx rimraf package-lock.json && npx -p rimraf rimraf node_modules && npm cache verify",
    "lint": "npx prettier-standard --lint",
    "nuke": "npm run clean && npm install",
    "start": "npx webpack serve --config webpack.dev.js",
    "upgrade": "npx upgradeps -n"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@reduxjs/toolkit": "^1.5.1",
    "i18next": "^20.2.4",
    "i18next-browser-languagedetector": "^6.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.15",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "dotenv": "^9.0.2",
    "dotenv-webpack": "^7.0.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.2",
    "style-loader": "^2.0.0",
    "upgradeps": "^1.4.3",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "author": "Luciano Ropero <lropero@gmail.com>",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "*": "npx prettier-standard --lint"
  },
  "private": true
}
